<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>cambiarContenido</servlet-name>
        <servlet-class>pe.edu.unmsm.sistemas.servlets.cambiarContenido</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cambiarContenido</servlet-name>
        <url-pattern>/cambiarContenido.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>sidebar</servlet-name>
        <servlet-class>pe.edu.unmsm.sistemas.servlets.sidebar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>pe.edu.unmsm.sistemas.servlets.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sidebar</servlet-name>
        <url-pattern>/sidebar.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>parametros</servlet-name>
        <servlet-class>pe.edu.unmsm.sistemas.servlets.parametros</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>parametros</servlet-name>
        <url-pattern>/parametros.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file> login.jsp </welcome-file>
    </welcome-file-list>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
